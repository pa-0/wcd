#: graphics.c:1314
msgid "NAVIGATE MODE:"
msgstr "NAVIGATIE MODUS:"

#: graphics.c:1315
msgid "h or <Left>       go left."
msgstr "h of <Links>      ga naar links."

#: graphics.c:1316
msgid "j or <Down>       go down."
msgstr "j of <Omlaag>     ga omlaag."

#: graphics.c:1317
msgid "k or <Up>         go up."
msgstr "k of <Omhoog>     ga omhoog."

#: graphics.c:1318
msgid "l or <Right>      go right."
msgstr "l of <Rechts>     ga naar rechts."

#: graphics.c:1319
msgid "* or v or <Space> go forward to dir with same name."
msgstr "* of v of <Spatie> ga vooruit naar map met dezelfde naam."

#: graphics.c:1320
msgid "# or p or <BS>    go backward to dir with same name."
msgstr "# of p of <BS>    ga achteruit naar map met dezelfde naam."

#: graphics.c:1321
msgid "^ or a            go to beginning of line."
msgstr "^ of a            ga naar begin van regel."

#: graphics.c:1322
msgid "$ or e            go to end of line."
msgstr "$ of e            ga naar einde van regel."

#: graphics.c:1323
msgid "1                 go to root dir."
msgstr "1                 ga naar eerste map."

#: graphics.c:1324
msgid "g or G            go to last dir."
msgstr "g of G            ga naar laatste map."

#: graphics.c:1325
msgid "f                 go page forward."
msgstr "f                 ga pagina vooruit."

#: graphics.c:1326
msgid "b                 go page backward."
msgstr "b                 ga pagina achteruit."

#: graphics.c:1327
msgid "u                 go half page up."
msgstr "u                 ga halve pagina omhoog."

#: graphics.c:1328
msgid "d                 go half page down."
msgstr "d                 ga halve pagina omlaag."

#: graphics.c:1329
msgid "<Esc> or q        Abort."
msgstr "<Esc> of q        Afbreken."

#: graphics.c:1330
msgid "/                 Search forward."
msgstr "/                 Zoek vooruit."

#: graphics.c:1331
msgid "?                 Search backward."
msgstr "?                 Zoek achteruit."

#: graphics.c:1332
