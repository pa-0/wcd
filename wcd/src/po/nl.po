msgid "# or p or <BS>    go backward to dir with same name."
msgstr "# of p of <BS>    ga achteruit naar map met dezelfde naam."

#: graphics.c:1460
msgid "^ or a            go to beginning of line."
msgstr "^ of a            ga naar begin van regel."

#: graphics.c:1461
msgid "$ or e            go to end of line."
msgstr "$ of e            ga naar einde van regel."

#: graphics.c:1462
msgid "1                 go to root dir."
msgstr "1                 ga naar eerste map."

#: graphics.c:1463
msgid "g or G            go to last dir."
msgstr "g of G            ga naar laatste map."

#: graphics.c:1464
msgid "f                 go page forward."
msgstr "f                 ga pagina vooruit."

#: graphics.c:1465
msgid "b                 go page backward."
msgstr "b                 ga pagina achteruit."

#: graphics.c:1466
msgid "u                 go half page up."
msgstr "u                 ga halve pagina omhoog."

#: graphics.c:1467
msgid "d                 go half page down."
msgstr "d                 ga halve pagina omlaag."

#: graphics.c:1468
msgid "<Esc> or q        Abort."
msgstr "<Esc> of q        Afbreken."

#: graphics.c:1469
msgid "/                 Search forward."
msgstr "/                 Zoek vooruit."

#: graphics.c:1470
msgid "?                 Search backward."
msgstr "?                 Zoek achteruit."

#: graphics.c:1471
msgid "n                 Repeat latest / or ? search."
msgstr "n                 Herhaal laatste / of ? zoekopdracht."

#: graphics.c:1472 graphics.c:1495
msgid "<Enter>           Select directory."
msgstr "<Enter>           Kies map."

#: graphics.c:1473 graphics.c:1496 graphics.c:1513
msgid "Press any key."
msgstr "Druk een toets."

#: graphics.c:1479
msgid "SEARCH MODE with wildcard and subdir support:"
msgstr "ZOEK MODUS met jokertekens en submap ondersteuning:"

#: graphics.c:1480
msgid "<Left>            go left."
msgstr "<Links>           ga naar links."

#: graphics.c:1481
msgid "<Down>            go down."
msgstr "<Omlaag>          ga omlaag."

#: graphics.c:1482
msgid "<Up>              go up."
msgstr "<Omhoog>          ga omhoog."

#: graphics.c:1483
msgid "<Right>           go right."
msgstr "<Rechts>          ga naar rechts."

#: graphics.c:1484
msgid "CTRL-v            go forward to dir with same name."
msgstr "CTRL-v            ga vooruit naar map met dezelfde naam."

#: graphics.c:1485
msgid "CTRL-p            go backward to dir with same name."
msgstr "CTRL-p            ga achteruit naar map met dezelfde naam."

#: graphics.c:1486
msgid "CTRL-a            go to beginning of line."
msgstr "CTRL-a            ga naar begin van regel."

#: graphics.c:1487
msgid "CTRL-e            go to end of line."
msgstr "CTRL-e            ga naar einde van regel."

#: graphics.c:1488
msgid "CTRL-g            go to last dir."
msgstr "CTRL-g            ga naar laatste map."

#: graphics.c:1489
msgid "CTRL-f            go page forward."
msgstr "CTRL-f            ga pagina vooruit."

#: graphics.c:1490
msgid "CTRL-b            go page backward."
msgstr "CTRL-b            ga pagina achteruit."

#: graphics.c:1491
msgid "CTRL-u            go half page up."
msgstr "CTRL-u            ga halve pagina omhoog."

#: graphics.c:1492
msgid "CTRL-d            go half page down."
msgstr "CTRL-d            ga halve pagina omlaag."

#: graphics.c:1493
msgid "<Esc> or CTRL-x   Abort SEARCH MODE."
msgstr "<Esc> of CTRL-x   ZOEK MODUS afbreken."

#: graphics.c:1494
msgid "CTRL-n            Repeat latest / or ? search."
msgstr "CTRL-n            Herhaal laatste / of ? zoekopdracht."

#: graphics.c:1502
msgid "ZOOMING:"
msgstr "ZOOMEN:"

#: graphics.c:1503
msgid "z or i or CTRL-i  zoom in."
msgstr "z of i of CTRL-i  zoom in."

#: graphics.c:1504
msgid "Z or o or CTRL-o  zoom out."
msgstr "Z of o of CTRL-o  zoom uit."

#: graphics.c:1505
msgid "c                 condense: fold current level."
msgstr "c                 huidig niveau vouwen."

#: graphics.c:1506
msgid "C                 condense: fold subdir level."
msgstr "C                 submap niveau vouwen."

#: graphics.c:1507
msgid "w                 condense: fold current and sub levels."
msgstr "w                 huidig en subniveaus vouwen."

#: graphics.c:1508
msgid "y or CTRL-y       uncondense: unfold current and sub levels."
msgstr "y of CTRL-y       huidig en subniveaus ontvouwen."

#: graphics.c:1509
msgid "r or CTRL-r       uncondense: unfold all directories."
msgstr "r of CTRL-r       alle mappen ontvouwen."

#: graphics.c:1510
msgid "-                 fold directory."
msgstr "-                 vouw map."

#: graphics.c:1511
msgid "+ or =            unfold directory."
msgstr "+ of =            ontvouw map."

#: graphics.c:1512
msgid "l or <Right>      unfold and go right."
msgstr "l of <Rechts>     ontvouwen en naar rechts gaan."

#: graphics.c:1516
msgid "Screenheight must be > 20 for help"
msgstr "Schermhoogte moet groter dan 20 zijn voor help"

