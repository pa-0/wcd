#: wcd.c:2126
#, c-format
msgid ""
"Usage: wcd [options] [directory]\n"
"\n"
msgstr ""
"Käyttö: wcd [vaihtoehtoja] [hakemisto]\n"
"\n"

#: wcd.c:2127
#, c-format
msgid "directory:  Name of directory to change to.\n"
msgstr "hakemisto:   hakemisotn nimi, johon siirtyä.\n"

#: wcd.c:2128
#, c-format
msgid ""
"            Wildcards *, ? and [SET] are supported.\n"
"\n"
msgstr ""
"             Villikortit *, ? ja [<kirjainten joukko>] ovat tuettuja\n"
"\n"

#: wcd.c:2130
#, c-format
msgid "options:\n"
msgstr "vaihtoehtoja:\n"

#: wcd.c:2131
#, c-format
msgid "  -a                      Add current path to treefile\n"
msgstr "  -a                      Lisää polku tiedostoon treefile\n"

#: wcd.c:2132
#, c-format
msgid ""
"  -aa                     Add current and all parent paths to treefile\n"
msgstr ""
"  -aa                     Lisään nykyinen ja kaikki polut yläpuolella "
"tiedostoon treefile\n"

#: wcd.c:2133
#, c-format
msgid "  -A PATH                 Add tree from PATH\n"
msgstr "  -A POLKU                Lisää puuhierarkia POLKU\n"

#: wcd.c:2134
#, c-format
msgid "  -b                      Ban current path\n"
msgstr "  -b                      Lisää polku mustalle listalle\n"

#: wcd.c:2135
#, c-format
msgid "  -c,  --direct-cd        direct CD mode\n"
msgstr "  -c,  --direct-cd        Suora CD moodi\n"

#: wcd.c:2136
#, c-format
msgid "  -d DRIVE                set DRIVE for stack & go files (DOS)\n"
msgstr ""
"  -d ASEMA                Aseta ASEMA toimintojen stack & go tiedostoille "
"(DOS)\n"

#: wcd.c:2137
#, c-format
msgid "  -e                      add current path to Extra treefile\n"
msgstr "  -e                      Lisää polku tiedostoon Extra treefile\n"

#: wcd.c:2138
#, c-format
msgid ""
"  -ee                     add current and all parent paths to Extra "
"treefile\n"
msgstr ""
"  -ee                     Lisää nykyinen ja kaikki polut yläpuolella "
"tiedostoono Extra treefile\n"

#: wcd.c:2139
#, c-format
msgid "  -E PATH                 add tree from PATH to Extra treefile\n"
msgstr ""
"  -E POLKU                Lisää puuhierarkia hakemistosta POLKU tiedostoon "
"Extra treefile\n"

#: wcd.c:2140
#, c-format
msgid "  -f FILE                 use extra treeFile\n"
msgstr "  -f TIEDOSTO             Käytä extra treeFile\n"

#: wcd.c:2141
#, c-format
msgid "  +f FILE                 add extra treeFile\n"
msgstr "  +f TIEDOSTO             Lisää extra treeFile\n"

#: wcd.c:2142
#, c-format
msgid "  -g                      Graphics\n"
msgstr "  -g                      Graafinen moodi\n"

#: wcd.c:2143
#, c-format
msgid "  -gd                     Graphics, dump tree\n"
msgstr "  -gd                     Graafinen moodi, tulosta puuhierarkia\n"

#: wcd.c:2144
#, c-format
msgid "  -G PATH                 set PATH Go-script\n"
msgstr "  -G POLKU                Aseta go-tiedoston POLKU\n"

#: wcd.c:2145
#, c-format
msgid "  -GN, --no-go-script     No Go-script\n"
msgstr "  -GN, --no-go-script     Poista go-tiedosto käytöstä\n"

#: wcd.c:2146
#, c-format
msgid "  -h,  --help             show this Help\n"
msgstr "  -h,  --help             Näytä tämä Help\n"

#: wcd.c:2149
#, c-format
msgid "  -i,  --ignore-case      Ignore case (default)\n"
msgstr ""
"  -i,  --ignore-case      Isot ja pienet kirjaimet - eivät merkityksellisiä "
"(oletus)\n"

#: wcd.c:2150
#, c-format
msgid "  +i,  --no-ignore-case   regard case\n"
msgstr ""
"  +i,  --no-ignore-case   Isot ja pienet kirjaimet - merkityksellisiä\n"

#: wcd.c:2152
#, c-format
msgid "  -i,  --ignore-case      Ignore case\n"
msgstr ""
"  -i,  --ignore-case      Isot ja pienet kirjaimet - eivät merkityksellisiä\n"

#: wcd.c:2153
#, c-format
msgid "  +i,  --no-ignore-case   regard case (default)\n"
msgstr ""
"  +i,  --no-ignore-case   Isot ja pienet kirjaimet - merkityksellisiä "
"(oletus)\n"

#: wcd.c:2156
#, c-format
msgid "  -I,  --ignore-diacritics     Ignore diacritics\n"
msgstr "  -I,  --ignore-diacritics     Ohita diakrittiset merkit\n"

#: wcd.c:2157
#, c-format
msgid "  +I,  --no-ignore-diacritics  Regard diacritics (default)\n"
msgstr ""
"  +I,  --no-ignore-diacritics  Ota huomioon diakriittiset merkit (oletus)\n"

