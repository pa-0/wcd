#: wcd.c:2251
#, c-format
msgid ""
"Usage: wcd [options] [directory]\n"
"\n"
msgstr ""
"Käyttö: wcd [valitsimet] [hakemisto]\n"
"\n"

#: wcd.c:2252
#, c-format
msgid "directory:  Name of directory to change to.\n"
msgstr "hakemisto:  Hakemiston nimi, johon siirtyä.\n"

