#: graphics.c:1608
msgid "/ = search forward,  ? = search backward,  : = help"
msgstr "/ = busca adelante,  ? = busca atrás,  : = ayuda"

#: graphics.c:1613
msgid "SEARCH: "
msgstr "BUSCA: "

#: graphics.c:1616
msgid "Search: "
msgstr "Busca: "

#: graphics.c:1632
msgid "NAVIGATE MODE:"
msgstr "MODO NAVEGACIÓN:"

#: graphics.c:1633
msgid "h or <Left>       go left."
msgstr "h o <Izquierda>   ir a la izquierda."

#: graphics.c:1634
msgid "j or <Down>       go down."
msgstr "j o <Abajo>       ir hacia abajo."

#: graphics.c:1635
msgid "k or <Up>         go up."
msgstr "k o <Arriba>      ir hacia arriba."

#: graphics.c:1636
msgid "l or <Right>      go right."
msgstr "l o <Derecha>     ir a la derecha."

#: graphics.c:1637
msgid "* or v or <Space> go forward to dir with same name."
msgstr "* o v o <Espacio> ir hacia adelante al directorio con el mismo nombre."

#: graphics.c:1638
msgid "# or p or <BS>    go backward to dir with same name."
msgstr "# o p o <BS>      ir hacia atrás al directorio con el mismo nombre."

#: graphics.c:1639
msgid "^ or a            go to beginning of line."
msgstr "^ o a             ir al inicio de la línea."

#: graphics.c:1640
msgid "$ or e            go to end of line."
msgstr "$ o e             ir al final de la línea."

#: graphics.c:1641
msgid "1                 go to root dir."
msgstr "1                 ir al directorio raiz."

#: graphics.c:1642
msgid "g or G            go to last dir."
msgstr "g o G             ir al último directorio."

#: graphics.c:1643
msgid "f                 go page forward."
msgstr "f                 avanzar página."

#: graphics.c:1644
msgid "b                 go page backward."
msgstr "b                 retroceder página."

#: graphics.c:1645
msgid "u                 go half page up."
msgstr "u                 avanzar media página."

#: graphics.c:1646
msgid "d                 go half page down."
msgstr "d                 retroceder media página."

#: graphics.c:1647
msgid "t                 switch centered mode on/off."
msgstr "t                 enciende/apaga el modo centrado."

#: graphics.c:1648
msgid "T                 toggle between line drawing and ASCII characters."
msgstr "T                 intercambia entre gráficas y caracteres ASCII."

#: graphics.c:1649
msgid "<Esc> or q        Abort."
msgstr "<Esc> o q         Aborta."

#: graphics.c:1650
msgid "/                 Search forward."
msgstr "/                 Busca hacia adelante."

#: graphics.c:1651
msgid "?                 Search backward."
msgstr "?                 Busca hacia atrás."

#: graphics.c:1652
msgid "n                 Repeat latest / or ? search."
msgstr "n                 Repetir la última búsqueda / o ?."

#: graphics.c:1653 graphics.c:1677
msgid "<Enter>           Select directory."
msgstr "<Intro>           Selecciona directorio."

#: graphics.c:1661
msgid "SEARCH MODE with wildcard and subdir support:"
msgstr "MODO BUSQUEDA con soporte de comodines y subdirectorios:"

#: graphics.c:1662
msgid "<Left>            go left."
msgstr "<Izquierda>       ir a la izquierda."

#: graphics.c:1663
msgid "<Down>            go down."
msgstr "<Abajo>           ir hacia abajo."

#: graphics.c:1664
msgid "<Up>              go up."
msgstr "<Arriba>          ir hacia arriba."

#: graphics.c:1665
msgid "<Right>           go right."
msgstr "<Derecha>         ir a la derecha."

#: graphics.c:1666
msgid "CTRL-v            go forward to dir with same name."
msgstr "CTRL-v            ir hacia delante al directorio con el mismo nombre."

#: graphics.c:1667
msgid "CTRL-p            go backward to dir with same name."
msgstr "CTRL-p            ir hacia atrás al directorio con el mismo nombre."

#: graphics.c:1668
msgid "CTRL-a            go to beginning of line."
msgstr "CTRL-a            ir al inicio de la línea."

#: graphics.c:1669
msgid "CTRL-e            go to end of line."
msgstr "CTRL-e            ir al final de la línea."

#: graphics.c:1670
msgid "CTRL-g            go to last dir."
msgstr "CTRL-g            ir al último directorio."

#: graphics.c:1671
msgid "CTRL-f            go page forward."
msgstr "CTRL-f            avanzar página."

#: graphics.c:1672
msgid "CTRL-b            go page backward."
msgstr "CTRL-b            retroceder página."

#: graphics.c:1673
msgid "CTRL-u            go half page up."
msgstr "CTRL-u            avanzar media página."

#: graphics.c:1674
msgid "CTRL-d            go half page down."
msgstr "CTRL-d            retroceder media página."

#: graphics.c:1675
msgid "<Esc> or CTRL-x   Abort SEARCH MODE."
msgstr "<Esc> o CTRL-x    Abortar MODO BUSQUEDA."

#: graphics.c:1676
msgid "CTRL-n            Repeat latest / or ? search."
msgstr "CTRL-n            Repetir la última búsqueda / o ?."

#: graphics.c:1684
msgid "ZOOMING:"
msgstr "ZOOMING:"

#: graphics.c:1685
msgid "z or i or CTRL-i  zoom in."
msgstr "z o i o CTRL-i    acercamiento."

#: graphics.c:1686
msgid "Z or o or CTRL-o  zoom out."
msgstr "Z o o o CTRL-o    alejamiento."

#: graphics.c:1687
msgid "c                 condense: fold current level."
msgstr "c                 condensar: condensar nivel actual."

#: graphics.c:1688
msgid "C                 condense: fold subdir level."
msgstr "C                 condensar: condensar nivel del subdirectorio."

#: graphics.c:1689
msgid "w                 condense: fold current and sub levels."
msgstr "w                 condensar: condensar nivel actual y subdirectorios."

#: graphics.c:1690
msgid "y or CTRL-y       uncondense: unfold current and sub levels."
msgstr "y o CTRL-y        expandir: expandir nivel actual y subdirectorios."

#: graphics.c:1691
msgid "r or CTRL-r       uncondense: unfold all directories."
msgstr "r o CTRL-r        expandir: expandir todos los directorios."

#: graphics.c:1692
msgid "-                 fold directory."
msgstr "-                 abrir directorio."

#: graphics.c:1693
msgid "+ or =            unfold directory."
msgstr "+ o =             cerrar directorio."

#: graphics.c:1694
msgid "l or <Right>      unfold and go right."
msgstr "l o <Derecha>     cerrar e ir a la derecha."

#: graphics.c:1698
msgid "Screenheight must be > 21 for help"
msgstr "La altura de la pantalla debe ser > 21 para la ayuda"

