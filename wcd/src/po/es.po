#: wcd.c:1550
#, c-format
msgid ""
"wcd  %s  (%s) - Wherever Change Directory\n"
"Usage: wcd [-a[a]] [-A <path>] [-b] [-c] [-d <drive>] [-e[e]] [-E <path>]\n"
"       [-f <treefile>] [-g[a|d]] [-G <path>] [-h] [-i] [-j] [-k] [-l "
"<alias>]\n"
"       [-[m|M] <dir>] [-n <path>] [-N] [-o[d]] [-q] [-[r|rmtree] <dir>] [-"
"s]\n"
"       [-S <path>] [-t] [-u <user>] [-v] [-V] [-w] [-x <path>] [-xf <file>]\n"
"       [-z #] [-[#]] [+[#]] [=] [drive:][dir]\n"
"\n"
"  [dir]   (partial) name of directory to change to.\n"
"          Wildcards *, ? and [SET] are supported!\n"
"\n"
msgstr ""
"wcd  %s  (%s) - Wherever Change Directory\n"
"Uso: wcd [-a[a]] [-A <ruta>] [-b] [-c] [-d <unidad>] [-e[e]][dir] [-E "
"<ruta>]\n"
"       [-f <arch. árbol>] [-g[a|d]] [-G <ruta>] [-h] [-i] [-j] [-k] [-l]\n"
"       [-[m|M] <directorio>] [-n <ruta>] [-N] [-o[d]] [-q] [-[r|rmtree] "
"<directorio>] [-s]\n"
"       [-S <ruta>] [-t] [-u <usuario>] [-v] [-V] [-w] [-x <ruta>] [-xf "
"<archivo>]\n"
"       [-z #] [-[#]] [+[#]] [=] [drive:][directorio]\n"
"\n"
"  [directorio] (parcial) nombre del directorio al que desea cambiarse.\n"
"               Se pueden emplear los comodines *, ? y [conjunto]\n"
"\n"

#: wcd.c:1561
#, c-format
msgid ""
"options:\n"
"  -a             Add current path to treefile\n"
"  -aa            Add current and all parent paths to treefile\n"
"  -A             Add tree from <path>\n"
"  -b             Ban current path\n"
"  -c             direct CD mode\n"
"  -d             set <drive> for stack & go files (DOS)\n"
"  -e             add current path to Extra treefile\n"
"  -ee            add current and all parent paths to Extra treefile\n"
"  -E             add tree from <path> to Extra treefile\n"
"  -f             use extra treeFile\n"
"  +f             add extra treeFile\n"
"  -g             Graphics\n"
"  -ga            Graphics, alternative navigation\n"
"  -gc            Graphics, centered\n"
"  -gd            Graphics, dump tree\n"
"  -G             set path Go-script\n"
"  -GN            No Go-script\n"
"  -h, --help     show this Help\n"
"  -i             Ignore case\n"
"  +i             Regard case\n"
"  -j             Just go mode\n"
"  -k             Keep paths\n"
"  -K             Colors\n"
"  -l             aLias current directory\n"
"  -L             print software License\n"
"  -m             Make <dir>, add to treefile\n"
"  -M             Make <dir>, add to extra treefile\n"
"  -n             use relative treefile in <path>\n"
"  +n             add relative treefile in <path>\n"
"  -N             use Numbers\n"
"  -o             use stdOut\n"
"  -od            dump matches\n"
"  -q             Quieter operation\n"
"  -r             Remove <dir>\n"
"  -rmtree        Remove <dir> recursive\n"
"  -s             Scan disk from $HOME\n"
"  -S             Scan disk from <path>\n"
"  +S             Scan disk from <path>, create relative treefile\n"
"  -u             use <user> treefile\n"
"  +u             add <user> treefile\n"
"  -v, --verbose  Verbose operation\n"
"  -V, --version  print Version info\n"
"  -w             Wild matching only\n"
"  -x             eXclude <path> during disk scan\n"
"  -xf            eXclude paths from <file>\n"
"  -z             set max stack siZe\n"
"  -              Push dir (# times)\n"
"  +              Pop dir (# times)\n"
"  =              Show stack\n"
msgstr ""
"options:\n"
"  -a             Añade la ruta actual al archivo de árbol\n"
"  -aa            Añade la ruta actual y sus padres al archivo de árbol\n"
"  -A             Añade árbol desde <ruta>\n"
"  -b             Prohíbe la ruta actual\n"
"  -c             Usa modo CD directo\n"
"  -d             Establece la <unidad> para almacenar los archivos de la "
"pila y el script go (DOS)\n"
"  -e             Añade la ruta actual al archivo de árbol extra\n"
"  -ee            Añade la ruta actual y sus padres al archivo de árbol "
"extra\n"
"  -E             Añade árbol desde <ruta> al archivo extra del árbol\n"
"  -f             Usa archivo de árbol extra\n"
"  +f             Añade archivo de árbol extra\n"
"  -g             Ambiente gráfico\n"
"  -ga            Ambiente gráfico, navegación alterna\n"
"  -gc            Ambiente gráfico, centrado\n"
"  -gd            Ambiente gráfico, impresión del árbol\n"
"  -G             Establece la ruta del script go\n"
"  -GN            No emplees el script go\n"
"  -h, --help     Despliega esta ayuda\n"
"  -i             Trata igual mayúsculas y minúsculas\n"
"  +i             Trata distinto mayúsculas y minúsculas\n"
"  -j             Modo directo\n"
"  -k             Retén las rutas\n"
"  -K             Colores\n"
"  -l             Crea un alias para la ruta actual\n"
"  -L             Imprime la Licencia del Programa\n"
"  -m             Crea <directorio>, y añádelo al archivo de árbol\n"
"  -M             Crea <directorio>, y añádelo al archivo de árbol extra\n"
"  -n             Emplea archivo de árbol relativo en <ruta>\n"
"  +n             Añade archivo de árbol relativo en <ruta>\n"
"  -N             Emplea números\n"
"  -o             Emplea la salida estándar (stdout)\n"
"  -od            Imprime correspondencias\n"
"  -q             Operación silenciosa\n"
"  -r             Remueve <directorio>\n"
"  -rmtree        Remueve recursivamente <directorio>\n"
"  -s             Reexplora disco desde $HOME\n"
"  -S             Explora disco desde <ruta>\n"
"  +S             Explora disco desde <ruta>, crea archivo de árbol relativo\n"
"  -u             Emplea archivo de árbol de <usuario>\n"
"  +u             Añade archivo de árbol de <usuario>\n"
"  -v, --verbose  Imprime mensajes de información\n"
"  -V, --version  Imprime información sobre la versión\n"
"  -w             Emplea sólo concordancias inexactas\n"
"  -x             Excluye <ruta> durante la exploración del disco\n"
"  -xf            Excluye rutas contenidas en <archivo>\n"
"  -z             Establece el tamaño máximo para la pila\n"
"  -              Mete directorio a la pila (# veces)\n"
"  +              Saca directorio de la pila (# veces)\n"
"  =              Muestra la pila\n"

